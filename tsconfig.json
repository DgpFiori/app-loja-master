{
  "extends": "expo/tsconfig.base",
  "target": "es2015",
  "lib": ["es2015"],
  "compilerOptions": {
    "strict": true,
    "target": "es2015",
    "lib": ["es2015"],
    "paths": {
      "@/*": ["./*"]
    },
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowJs": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
